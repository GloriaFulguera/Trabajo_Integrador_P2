QLabel#lblCantidadCompra,QLabel#lblMonedaCompra  {
  color: #334155;
  font-weight: 600;
}

/* ==== ComboBox (dropdown) ==== */
QComboBox#cmbCurrencyCompra {
  background: #ffffff;
  color: #111827;
  border: 1px solid #cbd5e1;
  border-radius: 10px;
  padding: 8px 36px 8px 10px;  
  min-height: 34px;
}
QComboBox#cmbCurrencyCompra:focus {
  border: 2px solid #22c55e;     /* acento verde */
  background: #ffffff;
}
QComboBox#cmbCurrencyCompra::drop-down {
  border: 0; width: 24px;
}
QComboBox#cmbCurrencyCompra QAbstractItemView {
  background: #ffffff;
  color: #111827;
  border: 1px solid #cbd5e1;
  selection-background-color: #22c55e;
  selection-color: #ffffff;
  outline: 0;
}

/* ==== Botón principal ==== */
QPushButton#btnComprarMoneda {
  background: #16a34a;
  color: #ffffff;
  border: 1px solid #15803d;
  border-radius: 10px;
  padding: 10px 16px;
  font-weight: 600;
}
QPushButton#btnComprarMoneda:hover   { background: #17b455; }
QPushButton#btnComprarMoneda:pressed { background: #12803e; }
QPushButton#btnComprarMoneda:disabled{
  background: #e5f7ed; color: #065f46; border-color: #bfead3;
}

/* ===== Campos ===== */
QLineEdit#txtCantidadCompra {
  background: #f8fafc;
  color: #111827;
  border: 1px solid #cbd5e1;
  border-radius: 10px;
  padding: 10px 12px;
}
QLineEdit#txtCantidadCompra:focus{
  border: 2px solid #22c55e;
  background: #ffffff;
}
QLineEdit#txtCantidadCompra::placeholder, QLineEdit#txtPassword::placeholder {
  color: #94a3b8;
}</string>
