   <string notr="true">/* ===== Fondo general ===== */
QMainWindow#MainWindow,
QWidget#centralwidget {
  background: qlineargradient(x1:0,y1:0,x2:1,y2:1, stop:0 #0b1210, stop:1 #0f1c17);
  color: #E8EEF7;
  font-family: &quot;Segoe UI&quot;,&quot;Inter&quot;,Arial;
}

QStackedWidget#stackMain {
  background: #ffffff;
  border: 1px solid #ffffff;
  border-radius: 10px;
}

/* ===== Título superior (bienvenida) ===== */
QLabel#lblWelcomeUser {
  color: #ffffff;
  font-size: 22px;
  font-weight: 800;
  letter-spacing: .3px;
  border: none;
  padding: 4px 0 8px 0;
}

/* =========================================================
   TÍTULOS DE SECCIÓN (todas las páginas)
   ======================================================= */
QLabel#lblTituloCrearCuentas_3,
QLabel#lblTituloComprarMoneda,
QLabel#lblTituloVenderMoneda,
QLabel#lblTituloDepositarMoneda {
  color: #0f172a;
  font-size: 26px;
  font-weight: 800;
  letter-spacing: .3px;
  border: none;
  padding: 4px 0 8px 0;
}

/* =========================================================
   LABELS DE CAMPO (todas las páginas)
   ======================================================= */
QLabel#lblMoneda_3,
QLabel#lblCantidadCompra,
QLabel#lblMonedaCompra,
QLabel#lblMonedaVender,
QLabel#lblCantidadVender,
QLabel#lblCantidadDeposito {
  color: #334155;
  font-weight: 600;
}

/* =========================================================
   BOTONES PRINCIPALES &quot;VERDES&quot;
   (se agrupan y se mantiene override para btnCerrar:disabled)
   ======================================================= */
QPushButton#btnCerrar,
QPushButton#btnCreateAccount_3,
QPushButton#btnPurchase,
QPushButton#btnSell,
QPushButton#btnDeposit {
  background: #16a34a;
  color: #ffffff;
  border: 1px solid #15803d;
  border-radius: 10px;
  padding: 10px 16px;
  font-weight: 600;
}
QPushButton#btnCerrar:hover,
QPushButton#btnCreateAccount_3:hover,
QPushButton#btnPurchase:hover,
QPushButton#btnSell:hover,
QPushButton#btnDeposit:hover   { background: #17b455; }

QPushButton#btnCerrar:pressed,
QPushButton#btnCreateAccount_3:pressed,
QPushButton#btnPurchase:pressed,
QPushButton#btnSell:pressed,
QPushButton#btnDeposit:pressed { background: #12803e; }

/* disabled general para los verdes (paginas); luego override para btnCerrar */
QPushButton#btnCreateAccount_3:disabled,
QPushButton#btnPurchase:disabled,
QPushButton#btnSell:disabled,
QPushButton#btnDeposit:disabled {
  background: #e5f7ed;
  color: #065f46;
  border-color: #bfead3;
}
/* override: btnCerrar conserva su disabled original diferente */
QPushButton#btnCerrar:disabled {
  background: #a7f3d0;
  color: #065f46;
  border-color: #86efac;
}

/* =========================================================
   BOTONES SECUNDARIOS (menú)
   ======================================================= */
QPushButton#btnCreateAccountM,
QPushButton#btnDepositM,
QPushButton#btnPurchaseM,
QPushButton#btnSellM {
  background: #ffffff;
  color: #0b3d2e;
  border: 1px solid #94a3b8;
  border-radius: 10px;
  padding: 10px 16px;
  font-weight: 600;
}
QPushButton#btnCreateAccountM:hover,
QPushButton#btnDepositM:hover,
QPushButton#btnPurchaseM:hover,
QPushButton#btnSellM:hover {
  background: #16a34a;
  border-color: #ffffff;
  color: #ffffff;
}

/* =========================================================
   COMBOBOX (todas las páginas)
   ======================================================= */
QComboBox#cmbCurrency_3,
QComboBox#cmbPurchaseCurrency,
QComboBox#cmbSellCurrency {
  background: #ffffff;
  color: #111827;
  border: 1px solid #cbd5e1;
  border-radius: 10px;
  padding: 8px 36px 8px 10px;  /* espacio para flecha */
  min-height: 34px;
}
QComboBox#cmbCurrency_3:focus,
QComboBox#cmbPurchaseCurrency:focus,
QComboBox#cmbSellCurrency:focus {
  border: 2px solid #22c55e;
  background: #ffffff;
}
QComboBox#cmbCurrency_3::drop-down,
QComboBox#cmbPurchaseCurrency::drop-down,
QComboBox#cmbSellCurrency::drop-down {
  border: 0; width: 24px;
}
QComboBox#cmbCurrency_3 QAbstractItemView,
QComboBox#cmbPurchaseCurrency QAbstractItemView,
QComboBox#cmbSellCurrency QAbstractItemView {
  background: #ffffff;
  color: #111827;
  border: 1px solid #cbd5e1;
  selection-background-color: #22c55e;
  selection-color: #ffffff;
  outline: 0;
}

/* =========================================================
   LINE EDITS (todas las páginas)
   ======================================================= */
QLineEdit#txtPurchaseAmount,
QLineEdit#txtSellAmount,
QLineEdit#txtAmount {
  background: #f8fafc;
  color: #111827;
  border: 1px solid #cbd5e1;
  border-radius: 10px;
  padding: 10px 12px;
}
QLineEdit#txtPurchaseAmount:focus,
QLineEdit#txtSellAmount:focus,
QLineEdit#txtAmount:focus {
  border: 2px solid #22c55e;
  background: #ffffff;
}
QLineEdit#txtPurchaseAmount::placeholder,
QLineEdit#txtSellAmount::placeholder,
QLineEdit#txtAmount::placeholder,
QLineEdit#txtPassword::placeholder {
  color: #94a3b8;
}
