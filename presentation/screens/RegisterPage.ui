QFrame#cardRegister {
  background: #ffffff;
  border: 1px solid #d1d5db;
  border-radius: 14px;
  padding: 24px;
  max-width: 420px;
}

/* ===== Título ===== */
QLabel#lblTitleRegister {
  color: #0b3d2e;
  font-size: 30px;
  font-weight: 800;
  letter-spacing: .5px;
  border: none;
}

/* ===== Labels ===== */
